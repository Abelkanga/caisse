{% extends "base.html.twig" %}

    {% block title %}
        Caisse - Inscription
    {% endblock %}
    
    {% block body %}
        <div class="container">
            <h1 class="mt-4">Formulaire d'inscription</h1>
            
            <form method="post">
                {{ form_start(form) }}
                {{ form_widget(form) }}
                 <button type="submit" class="btn btn-primary mt-4">S'inscrire</button>
                {{ form_end(form) }}
            </form>

        </div>
    {% endblock %}
    
    
    
    
    
    