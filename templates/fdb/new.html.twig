{% extends 'base.html.twig' %}

{% block title %}Nouvelle fiche de besoin{% endblock %}

{% block body %}

    <div class="container-fluid">
        {{ form_start(form) }}
        <div class="card card-fluid">
            <div class="card-header bg-soft-custom">
                <h6>Créer une fiche de besoin</h6>
            </div>
            <div class="card-body">
                {{ include('fdb/_form.html.twig') }}
            </div>
            <div class="card-footer d-flex flex-column flex-md-row justify-content-end">
                <a href="{{ path('fdb_index') }}" class="btn btn-info mb-2 mb-md-0 mr-md-2">
                    <i class="oi oi-home"></i> Retour
                </a>
                <button type="submit" class="btn btn-primary">Enregistrer</button>
            </div>
        </div>
        {{ form_end(form) }}
    </div>

{% endblock %}

